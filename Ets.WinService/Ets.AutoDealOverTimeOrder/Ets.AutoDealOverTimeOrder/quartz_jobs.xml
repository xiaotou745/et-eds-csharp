<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains job definitions in schema version 2.0 format -->

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <!--定时导出发送邮件-->
    <job>
      <name>AutoDealBLL</name>
      <group>AutoDealGroup</group>
      <description>AutoDealService</description>
      <job-type>Ets.AutoDealWithward.AutoDealBLL,Ets.AutoDealWithward</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <!--定时导出发送邮件-->
    <trigger>
      <cron>
        <name>AutoDealBLLTrigger</name>
        <group>AutoDealTriggerGroup</group>
        <job-name>AutoDealBLL</job-name>
        <job-group>AutoDealGroup</job-group>
        <cron-expression>* 1/3 * * * ?</cron-expression>
      </cron>
    </trigger>
  
  </schedule>
</job-scheduling-data>