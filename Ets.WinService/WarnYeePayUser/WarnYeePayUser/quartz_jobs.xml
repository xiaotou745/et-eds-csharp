<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains job definitions in schema version 2.0 format -->

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <!--定时导出发送邮件-->
    <job>
      <name>WarnBLL</name>
      <group>WarnDataGroup</group>
      <description>emailServices</description>
      <job-type>WarnYeePayUser.WarnBLL,WarnYeePayUser</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <!--定时导出发送邮件-->
    <trigger>
      <cron>
        <name>WarnBLLTrigger</name>
        <group>WarnDataTriggerGroup</group>
        <job-name>WarnBLL</job-name>
        <job-group>WarnDataGroup</job-group>
        <cron-expression>* * 0 ? * ?</cron-expression>
      </cron>
    </trigger>
  
  </schedule>
</job-scheduling-data>