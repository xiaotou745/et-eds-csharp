<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 				version="2.0">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <job>
      <name>Job01</name>
      <group>Group01</group>
      <description></description>
      <job-type>Ets.CrossShopShortMessage.BLL.Job_ShortMessage,Ets.CrossShopShortMessage</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Trigger01</name>
        <group>TriggerGroup01</group>
        <description></description>
        <job-name>Job01</job-name>
        <job-group>Group01</job-group>
        <cron-expression>59 59 23 * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>