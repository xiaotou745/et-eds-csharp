@using (Html.BeginForm("", "", FormMethod.Post, new {enctype = "multipart/form-data"}))
{
    <div class="SearchMd">
        <div style="margin-bottom: 10px;"><input id="file1" type="file" name="file1"><span style="color: red">(导入必须为excel，字段：骑士姓名、手机号、城市、身份证号 ，每次最多导入50条数据)</span>
            <a href="/Content/Template/物流公司批量导入配送员.xls" target="_blank">下载模板</a></div>
